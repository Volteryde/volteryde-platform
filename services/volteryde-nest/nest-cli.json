{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/volteryde-api/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/volteryde-api/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/volteryde-api",
  "projects": {
    "volteryde-api": {
      "type": "application",
      "root": "apps/volteryde-api",
      "entryFile": "main",
      "sourceRoot": "apps/volteryde-api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/volteryde-api/tsconfig.app.json"
      }
    },
    "telematics": {
      "type": "application",
      "root": "apps/telematics",
      "entryFile": "main",
      "sourceRoot": "apps/telematics/src",
      "compilerOptions": {
        "tsConfigPath": "apps/telematics/tsconfig.app.json"
      }
    },
    "fleet": {
      "type": "application",
      "root": "apps/fleet",
      "entryFile": "main",
      "sourceRoot": "apps/fleet/src",
      "compilerOptions": {
        "tsConfigPath": "apps/fleet/tsconfig.app.json"
      }
    },
    "charging": {
      "type": "application",
      "root": "apps/charging",
      "entryFile": "main",
      "sourceRoot": "apps/charging/src",
      "compilerOptions": {
        "tsConfigPath": "apps/charging/tsconfig.app.json"
      }
    },
    "booking": {
      "type": "application",
      "root": "apps/booking",
      "entryFile": "main",
      "sourceRoot": "apps/booking/src",
      "compilerOptions": {
        "tsConfigPath": "apps/booking/tsconfig.app.json"
      }
    },
    "notifications": {
      "type": "application",
      "root": "apps/notifications",
      "entryFile": "main",
      "sourceRoot": "apps/notifications/src",
      "compilerOptions": {
        "tsConfigPath": "apps/notifications/tsconfig.app.json"
      }
    }
  }
}
