{
  "name": "@volteryde/nest",
  "version": "1.0.0",
  "description": "NestJS backend - Telematics, Booking, Fleet Operations, Charging",
  "main": "dist/services/volteryde-nest/src/main.js",
  "scripts": {
    "dev": "nest build && node dist/services/volteryde-nest/src/main",
    "build": "nest build",
    "start": "node dist/services/volteryde-nest/src/main",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/services/volteryde-nest/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@aws-sdk/client-timestream-query": "^3.931.0",
    "@aws-sdk/client-timestream-write": "^3.931.0",
    "@nestjs/common": "^10.4.20",
  "@nestjs/core": "^10.4.20",
  "@nestjs/config": "^3.3.0",
  "@nestjs/platform-express": "^10.4.20",
  "@nestjs/platform-socket.io": "^10.4.20",
  "@nestjs/swagger": "^7.4.2",
  "@nestjs/typeorm": "^10.0.2",
  "@nestjs/websockets": "^10.4.20",
    "@temporalio/client": "^1.8.0",
    "@types/geojson": "^7946.0.16",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "firebase-admin": "^13.6.0",
    "ioredis": "^5.8.2",
    "mqtt": "^4.3.7",
    "ngeohash": "^0.6.3",
    "pg": "^8.11.0",
    "redis": "^4.6.7",
    "rxjs": "^7.8.1",
    "socket.io": "^4.8.1",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
"@nestjs/schematics": "^10.0.0",
"@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.4",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
